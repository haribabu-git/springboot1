node{
    stage('SCM Checkout'){
        git 'https://github.com/haribabu-git/springboot1'    
    }
    stage('Compile-package'){
        dir(springboot1/SpringBoot){
            def mvnHome = tool name: 'MAVEN_HOME', type: 'maven';
            bat "${mvnHome}/bin/mvn clean"
        }         
    }
    
}
